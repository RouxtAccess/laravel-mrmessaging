{
  "name": "rouxtaccess/laravel-mrmessaging",
  "description": "SMS Portal Notification Channel for laravel",
  "keywords": ["laravel", "notifications", "Mr Messaging"],
  "license": "MIT",
  "authors": [
    {
      "name": "John Roux",
      "email": "john.roux1@gmail.com",
      "role": "Developer"
    }],
  "require": {
    "php" : ">=8.2",
    "illuminate/config": ">=10.0",
    "illuminate/notifications": ">=10.0",
    "illuminate/http": ">=10.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "mockery/mockery": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "Illuminate\\Notifications\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Illuminate\\Tests\\Notifications\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers" : [
        "Illuminate\\Notifications\\MrMessagingServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
